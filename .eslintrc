{
  extends: "eslint:recommended",

  env: {
    "browser": true,
    "node": true,
    "mocha": true
  },

  rules: {
    "strict": 0,
    "quotes": [2, "single"],
    "indent": [2, 2, {SwitchCase: 1}],
    "semi": [2, "always"],
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "new-cap": 0,
    "no-extra-boolean-cast": 0,
    "yoda": 0,
    "no-empty": 0,
    "no-use-before-define": 0,
    "camelcase": 0,
    "object-curly-spacing": 0,
    "array-bracket-spacing": 0,
    "max-len": 0,
    "comma-dangle": [2, "never"],
    "space-before-function-paren": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-console": 0,
    "no-void": 0,
    "func-names": 0,
    "no-nested-ternary": 0,
    "quote-props": [2, "consistent-as-needed", { "keywords": false, "unnecessary": true, "numbers": false }],
    "space-infix-ops": 0,
    "prefer-const": 0,
    "prefer-template": 0,
    "spaced-comment": 0,
    "prefer-rest-params": 0
  },
  "globals": {
    "expect": true,
    "SoundCloudAudio": true
  }
}
